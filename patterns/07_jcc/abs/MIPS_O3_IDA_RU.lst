my_abs:
; перейти если \$a0<0:
                bltz    $a0, locret_10
; просто вернуть входное значение (\$a0) в \$v0:
                move    $v0, $a0
                jr      $ra
                or      $at, $zero ; branch delay slot, NOP
locret_10:
; поменять у значения знак и сохранить его в \$v0:
                jr      $ra
; это псевдоинструкция. на самом деле, это "subu \$v0,\$zero,\$a0" (\$v0=0-\$a0)
                negu    $v0, $a0
