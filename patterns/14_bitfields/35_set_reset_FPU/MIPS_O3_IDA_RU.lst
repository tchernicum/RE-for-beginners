my_abs:
; скопировать из сопроцессора 1:
                mfc1    $v1, $f12
                li      $v0, 0x7FFFFFFF
; \$v0=0x7FFFFFFF
; применить И:
                and     $v0, $v1
; скопировать в сопроцессор 1:
                mtc1    $v0, $f0
; возврат
                jr      $ra
                or      $at, $zero ; branch delay slot

set_sign:
; скопировать из сопроцессора 1:
                mfc1    $v0, $f12
                lui     $v1, 0x8000
; \$v1=0x80000000
; применить ИЛИ:
                or      $v0, $v1, $v0
; скопировать в сопроцессор 1:
                mtc1    $v0, $f0
; возврат
                jr      $ra
                or      $at, $zero ; branch delay slot

negate:
; скопировать из сопроцессора 1:
                mfc1    $v0, $f12
                lui     $v1, 0x8000
; \$v1=0x80000000
; применить исключающее ИЛИ:
                xor     $v0, $v1, $v0
; скопировать в сопроцессор 1:
                mtc1    $v0, $f0
; возврат
                jr      $ra
                or      $at, $zero ; branch delay slot
