f:
; \$a0=a
; rt variable will reside in \$v0:
                move    $v0, $zero
; i variable will reside in \$v1:
                move    $v1, $zero
                li      $t0, 1
                li      $a3, 32
                sllv    $a1, $t0, $v1
; \$a1 = \$t0<<\$v1 = 1<<i

loc_14:
                and     $a1, $a0
; \$a1 = a\&(1<<i)
; increment i:
                addiu   $v1, 1
; jump to loc\_28 if a\&(1<<i)==0 and increment rt:
                beqz    $a1, loc_28
                addiu   $a2, $v0, 1
; if BEQZ was not triggered, save updated rt into \$v0:
                move    $v0, $a2

loc_28:
; if i!=32, jump to loc\_14 and also prepare next shifted value:
                bne     $v1, $a3, loc_14
                sllv    $a1, $t0, $v1
; return
                jr      $ra
                or      $at, $zero ; branch delay slot, NOP
