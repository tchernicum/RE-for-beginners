f:
; \$a0=a
; переменная rt будет находиться в \$v0:
                move    $v0, $zero
; переменная i будет находиться в \$v1:
                move    $v1, $zero
                li      $t0, 1
                li      $a3, 32
                sllv    $a1, $t0, $v1
; \$a1 = \$t0<<\$v1 = 1<<i

loc_14:
                and     $a1, $a0
; \$a1 = a\&(1<<i)
; инкремент i:
                addiu   $v1, 1
; переход на loc\_28 если a\&(1<<i)==0 и инкремент rt:
                beqz    $a1, loc_28
                addiu   $a2, $v0, 1
; если BEQZ не сработала, сохранить обновленную rt в \$v0:
                move    $v0, $a2

loc_28:
; если i!=32, перейти на loc\_14 а также подготовить следующее сдвинутое значение:
                bne     $v1, $a3, loc_14
                sllv    $a1, $t0, $v1
; возврат
                jr      $ra
                or      $at, $zero ; branch delay slot, NOP
